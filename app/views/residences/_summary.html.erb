<ul class="list-group">
  <%= render 'maybe', attribute: "From", object: residence.start %>
  <%= render 'maybe', attribute: "To", object: residence.end_or_current %>
  <%= render 'maybe', attribute: "At", object: residence.address.full %>
  <%= render 'maybe', attribute: "Rent", object: number_to_currency(residence.rent) %>
</ul>

<div class="panel-heading">
Landlord
</div>

<ul class="list-group">
  <%= render 'maybe', attribute: "Name", object: residence.landlord.full_name %>
  <%= render 'maybe', attribute: "Address", object: residence.landlord.mail_address.full %>
  <% %w(cell_phone home_phone work_phone email).each do |attribute_name| %>
    <%= render 'maybe', attribute: attribute_name.to_s.humanize, object: residence.landlord[attribute_name] %>
  <% end %>
</ul>
