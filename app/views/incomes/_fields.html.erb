<div class="row">
  <div class="col-md-4">
    <%= f.collection_select :income_type_id, IncomeType.all, :id, :label, include_blank: true %>
  </div>

  <div class="col-md-4">
    <%= f.text_field :amount, label: "Yearly amount" %>
  </div>
  <div class="col-md-4">
    <%= f.collection_select :person_id, @current_applicant.household_members_including_self, :id, :description, label: "Who earns it?" %>
  </div>
</div>
